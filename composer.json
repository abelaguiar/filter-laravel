{
    "name": "abelaguiar/filter-laravel",
    "description": "Make filters in laravel",
    "type": "library",
    "keywords": ["filter", "laravel"],
    "license": "MIT",
    "authors": [
        {
            "name": "Abel Aguiar",
            "email": "abel.prog@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "php": "^7.0",
        "illuminate/http": "~5.*",
        "illuminate/support": "~5.*",
        "illuminate/console": "~5.*",
        "illuminate/database": "~5.*"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.0"
    },
    "autoload": {
        "psr-4": {
            "AbelAguiar\\Filter\\": "src"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "AbelAguiar\\Filter\\FilterServiceProvider"
            ]
        }
    }
}
